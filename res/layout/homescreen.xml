<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="#fff" >

     <LinearLayout 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:gravity="center">
    <LinearLayout 

android:background="@drawable/logo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    
        </LinearLayout>
        </LinearLayout> 
    
     <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:textColor="#555555"
                    android:gravity="center|left"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:id="@+id/gymid"
                    android:text="@string/gymid"
                    android:background="@drawable/gymidbg"/> 
    
     
        <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
    
                <LinearLayout 
                    android:layout_height="wrap_content"
                    android:layout_width="fill_parent"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/gymidbg"
                    android:id="@+id/reviewback"
                    android:gravity="center"> 
                <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:text="Feedback"
                    android:padding="6dp"
                    android:clickable="true"
                    android:textColor="#ffffff"
                     android:id="@+id/review"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/daybutton"/> 
                    </LinearLayout>
      <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:background="@drawable/contentbg"
        android:layout_marginLeft="20dp"
         android:layout_marginRight="20dp"
          android:layout_marginBottom="10dp"
          android:layout_marginTop="10dp"
        android:layout_above="@id/reviewback">
     
        
            
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">
        
            <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:background="@drawable/redbg"
        android:gravity="center"
        android:layout_weight=".6">
        
                <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".3">
        
                <Button 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/qrscanner"
                    android:layout_margin="5dp"
                    android:id="@+id/toscanfield"/>
                </LinearLayout>
                
                <TextView 
                    android:layout_height="fill_parent"
                    android:layout_width="wrap_content"
                    android:text="@string/scan"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_weight=".7"
                    android:textSize="18sp"/>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".4">
    
               <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:layout_margin="5dp"
                    android:textColor="#fff"
                    android:padding="4dp"
                    android:id="@+id/workout"
                    android:background="@drawable/worklog"/> 
        
            
                    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5"
        android:baselineAligned="false">
                <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:gravity="center"
        android:baselineAligned="false"
         android:layout_weight=".5">
                 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
        
                <Button 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:background="@drawable/clock"
                     android:id="@+id/toclass"/>
                 </LinearLayout>
                 
                      <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
        
                          
                <Button 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/mail"
                    android:id="@+id/tomessgelist"/>
                    <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:text=" 1 "
                     android:id="@+id/number"
                     android:padding="3dp"
                     android:visibility="gone"
                     android:layout_alignRight="@id/tomessgelist"
                     android:background="@drawable/circleback"/>
                  
            </RelativeLayout>
    </LinearLayout>
            <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:gravity="center|top"
        android:layout_weight=".5"
        android:baselineAligned="false">
                 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
                
                <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:text="@string/cs"
                     android:textColor="#555555"/>
                 </LinearLayout>
                 
                      <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
                
                <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:text="@string/ms"
                     android:textColor="#555555"/>
                 </LinearLayout>
    </LinearLayout>
    </LinearLayout>
    
                      <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:gravity="center|top"
        android:layout_weight=".5"
        android:baselineAligned="false">
                 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:gravity="center"
         android:layout_weight=".5">
                 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
        
                <Button 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/man"
                    android:id="@+id/logoutbutton"/>
                 </LinearLayout>
                 
                      <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
        
                <Button 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/dumble"
                    android:id="@+id/toroutine"/>
                 </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:gravity="center|top"
         android:layout_weight=".5">
                 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
                <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:text="@string/cg"
                     android:textColor="#555555"/>
                 </LinearLayout>
                                                             
                      <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_weight=".5">
                <TextView 
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                     android:text="@string/ro"
                     android:textColor="#555555"/>
                 </LinearLayout>
  </LinearLayout>
  </LinearLayout>
    </LinearLayout>

</LinearLayout>
</LinearLayout>
</RelativeLayout>
</LinearLayout>